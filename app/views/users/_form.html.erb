<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :favorite_genre %>
  <%= f.text_field :favorite_genre %>
  <%= f.label :favorite_books %>
  <%= f.text_field :favorite_books %>
  <%= f.label :about_me %>
  <%= f.text_area :about_me %>
  <%= f.submit %>
<% end %>
